{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "/Applications/Arduino.app/Contents/Java/hardware/**",
                "/Applications/Arduino.app/Contents/Java/hardware/arduino/avr/cores/**",
                "/Applications/Teensyduino.app/Contents/Java/hardware/teensy/avr/libraries/**"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64",
            "compilerArgs": [
                "-DARDUINO=10813",
                "-DESP32"
            ]
        }
    ],
    "version": 4
}